<p style="color: green"><%= notice %></p>

<h1>All rooms in our company</h1>

<div id="rooms">
  <% @rooms.each do |room| %>
    <%= render room %>
    <p>
      <%= link_to "Show this room", room %>
    </p>
  <% end %>
</div>
<% if user_signed_in? %>
  <% if current_user.isAdmin? %>
    <%= link_to "Add new room", new_room_path %>
  <% end %>
<% end %>